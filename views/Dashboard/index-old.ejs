<%- contentFor('body') %>

<h1>Selamat Datang, <%= data[0].fullname %></h1>
<br /><br />
<div class="row">
    <div class="col-xl-8">
        <div class="row">
            <div class="col-md-4">
                <div class="card">
                    <div class="card-body">
                        <div class="d-flex">
                            <div class="flex-1 overflow-hidden">
                                <p class="text-truncate font-size-14 mb-2">
                                    Total Mustahik
                                </p>
                                <h4 class="mb-0">
                                    <%= report1[0].total_mustahik %>
                                </h4>
                            </div>
                            <div class="text-primary ms-auto">
                                <i class="ri-stack-line font-size-24"></i>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-4">
                <div class="card">
                    <div class="card-body">
                        <div class="d-flex">
                            <div class="flex-1 overflow-hidden">
                                <p class="text-truncate font-size-14 mb-2">
                                    Total Santunan
                                </p>
                                <h4 class="mb-0" id="total-santunan"></h4>
                            </div>
                            <div class="text-primary ms-auto">
                                <i class="ri-store-2-line font-size-24"></i>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-4">
                <div class="card">
                    <div class="card-body">
                        <div class="d-flex">
                            <div class="flex-1 overflow-hidden">
                                <p class="text-truncate font-size-14 mb-2">
                                    Jumlah Program
                                </p>
                                <h4 class="mb-0">
                                    <%= report1[0].jumlah_program %>
                                </h4>
                            </div>
                            <div class="text-primary ms-auto">
                                <i class="ri-briefcase-4-line font-size-24"></i>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- end row -->

        <div class="card">
            <div class="card-body">
                <h4 class="card-title mb-4">Santunan By Month This Year</h4>
                <div>
                    <div id="report-bar" class="apex-charts" dir="ltr"></div>
                </div>
            </div>
        </div>
    </div>

    <div class="col-xl-4">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title mb-4">Santunan by Program</h4>

                <div id="dashboard-donut" class="apex-charts"></div>
                <div class="row">
                    <% report2.forEach((item, index) => { %>
                    <div class="col-6">
                        <div class="text-center mt-4">
                            <p class="mb-2 text-truncate">
                                <i
                                    class="mdi mdi-circle text-primary font-size-10 me-1"
                                ></i>
                                <%= item.program %>
                            </p>
                            <h5><%= item.persentasi %> %</h5>
                        </div>
                    </div>
                    <% }); %>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- end row -->

<div class="row">
